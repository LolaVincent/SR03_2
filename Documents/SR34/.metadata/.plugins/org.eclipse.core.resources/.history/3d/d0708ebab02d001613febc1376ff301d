package beans;

import beans.*;

import java.util.List;
import java.util.ArrayList;

public class Categorie {

	private int id;
	private String sujet;	
	
	public void setId( long i ) {
        this.id = i;
    }

    public Long getId() {
        return id;
    }
		
	public void setSujet( String sujet ) {
        this.sujet= sujet;
    }
	
	public String getSujet() {
        return sujet;
    }
	
}